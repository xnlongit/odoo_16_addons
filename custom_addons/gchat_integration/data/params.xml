<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        
        <!-- System parameters for Google Chat integration -->
        <record id="param_gchat_webhook_url" model="ir.config_parameter">
            <field name="key">gchat.webhook.url</field>
            <field name="value">/gchat/webhook</field>
        </record>
        
        <record id="param_gchat_webhook_timeout" model="ir.config_parameter">
            <field name="key">gchat.webhook.timeout</field>
            <field name="value">30</field>
        </record>
        
        <record id="param_gchat_max_retries" model="ir.config_parameter">
            <field name="key">gchat.max.retries</field>
            <field name="value">3</field>
        </record>
        
        <record id="param_gchat_retry_delay" model="ir.config_parameter">
            <field name="key">gchat.retry.delay</field>
            <field name="value">5</field>
        </record>
        
        <record id="param_gchat_event_retention_days" model="ir.config_parameter">
            <field name="key">gchat.event.retention.days</field>
            <field name="value">30</field>
        </record>
        
    </data>
</odoo> 